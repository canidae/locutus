[% INCLUDE header.tmpl
  title = 'Files'
%]
    <p>
      <input type="text" value="filter files here" />
    </p>
[% IF files %]
    <ul>
  [% FOREACH file IN files %]
      <li><a href="file.pl?fiid=[% file.file_id %]">[% file.filename %]</a></li>
  [% END %]
    </ul>
[% END %]
[% INCLUDE footer.tmpl %]
